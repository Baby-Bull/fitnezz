{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "baseUrl": "./",
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "src/*"
      ],
      "icons/*": [
        "src/components/atoms/icons/*"
      ],
      "libs/*": [
        "src/libs/*"
      ],
      "Template/*": [
        "src/components/Template/*"
      ],
      "Organism/*": [
        "src/components/Organism/*"
      ],
      "Molecules/*": [
        "src/components/Molecules/*"
      ],
      "Atoms/*": [
        "src/components/Atoms/*"
      ],
      "Modal/*": [
        "src/components/Modal/*"
      ],
      "CustomHooks": [
        "src/components/CustomHooks"
      ],
      "ComponentSelectors": [
        "src/components/Selectors"
      ],
      "constants/*": [
        "src/constants/*"
      ],
      "libs": [
        "src/libs"
      ],
      "img": [
        "src/img"
      ],
      "testData": [
        "src/testData"
      ]
    },
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules"]
}
